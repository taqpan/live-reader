{
  "name": "live-reader",
  "version": "0.3.0",
  "description": "Simple feed reader for Firefox, yet another Live Bookmarks.",
  "repository": "https://github.com/taqpan/live-reader",
  "author": "taqpan",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dist": "sh ./ops/dist.sh",
    "build": "rm -rf ./build && webpack --mode production --config webpack.config.prod.js",
    "dev": "webpack --mode development --config webpack.config.dev.js",
    "watch": "webpack --mode development --watch --config webpack.config.dev.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/*.test.ts"
    ]
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "lodash": "^4.17.15",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^8.4.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.95",
    "@types/jest": "^25.1.2",
    "@types/lodash": "^4.14.149",
    "@types/webpack": "^4.41.6",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "@vue/eslint-config-typescript": "^5.0.1",
    "@vue/test-utils": "^1.0.0-beta.31",
    "autoprefixer": "^9.7.4",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.1.2",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.11",
    "webextension-polyfill-ts": "^0.12.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2"
  }
}
