{
  "root": true,
  "extends": "eslint:recommended",
  "rules": {
    "indent": [2, 4],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"]
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2017
  },
  "globals": {
    "browser": true
  }
}
